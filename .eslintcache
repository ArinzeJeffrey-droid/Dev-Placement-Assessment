lter-find find-country\">\r\n                    <input type=\"text\" onChange={e => handleChange(e)} name=\"search\" value={search} placeholder=\"&nbsp;&nbsp;&nbsp;&#x1F50D;Find in list\" className=\"filter\"/>\r\n                </div>\r\n                <div className=\"filter-find\">\r\n                    <select disabled name=\"\" className=\"country filter\" id=\"\">\r\n                        <option disabled defaultValue>&nbsp;&nbsp;&nbsp;Country</option>\r\n                        <option value=\"United Kingdom\">UK</option>\r\n                        <option value=\"Australia\">Australia</option>\r\n                    </select>\r\n                </div>\r\n                <div className=\"filter-find\">\r\n                    {/* placeholder for toggle */}\r\n                </div>\r\n            </div>\r\n            <div className=\"users-container\">\r\n            { error ? (<Button404 eventHandler={recallAPI}/>) : loading ? (<Loader/>) : (\r\n                    <>\r\n                    <div style={display ? css : { transition: \"opacity 1s ease-out\", opacity: 1}} className=\"inner\">\r\n                        {handleSearch().map((user,index) => (\r\n                            <UserCard key={index} details={user}/>\r\n                        ))}\r\n                    </div>\r\n                    <UserProfile/>\r\n                    </>\r\n                )\r\n            }\r\n            </div>\r\n            <Footer/>\r\n        </div>\r\n    </div>\r\n</div>\r\n    );\r\n}\r\n\r\nconst mapStateToProps = state => ({\r\n    users: state.users.users,\r\n    error: state.users.error,\r\n    loading: state.users.loading,\r\n    api_url: state.users.api_url,\r\n    header_title: state.users.header_title,\r\n    display: state.users.display\r\n})\r\n\r\nexport default connect(mapStateToProps)(Dashboard);","C:\\Users\\ABC\\Documents\\decagon-challenge\\src\\components\\reuseables\\UserProfile.jsx",[],"C:\\Users\\ABC\\Documents\\decagon-challenge\\s